{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "main.py"
        }
    ],
    "functions": {
        "api/**/*.py": {
            "maxDuration": 60
        }
    }
}